{
    "extends": "airbnb",
    "rules": {
      "no-console": "off",
      "comma-dangle": "off",
      "quotes": "off",
      "react/prop-types": 0,
      "arrow-body-style": 0,
      "space-before-function-paren": 0,
      "linebreak-style": 0
    },
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6
      }
  }